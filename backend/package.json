{
  "name": "app-backend",
  "description": "Backend",
  "scripts": {
    "start": "nodemon --watch \"src/**/*.ts\" -e ts,json --exec \"npm run build && node ./dist/server.js\"",
    "test": "nodemon --exec \"cross-env NODE_ENV=test mocha ./src/**/*.test.js --exit || exit 1\"",
    "build": "tsc",
    "db:create": "npm run build && node ./dist/database/migrations/create"
  },
  "dependencies": {
    "@google-cloud/storage": "5.1.1",
    "@sendgrid/mail": "7.2.1",
    "aws-sdk": "2.713.0",
    "bcrypt": "5.0.0",
    "cli-highlight": "2.1.4",
    "cors": "2.8.5",
    "dotenv": "8.2.0",
    "express": "4.17.1",
    "formidable-serverless": "1.0.3",
    "helmet": "3.23.3",
    "jsonwebtoken": "8.5.1",
    "lodash": "4.17.21",
    "moment": "2.29.4",
    "mysql2": "2.1.0",
    "pg": "8.3.0",
    "pg-hstore": "2.3.3",
    "sequelize": "6.3.0",
    "sqlite": "4.0.11",
    "stripe": "8.69.0"
  },
  "private": true,
  "devDependencies": {
    "copyfiles": "2.3.0",
    "cross-env": "7.0.2",
    "mocha": "8.0.1",
    "node-mocks-http": "1.8.1",
    "nodemon": "2.0.4",
    "sequelize-cli": "6.2.0",
    "typescript": "3.9.6"
  }
}
